    ERRCONTADOR19=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 90)
    ERRCONTADOR20=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 91)
    ERRCONTADOR21=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 92)
    ERRCONTADOR22=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 93)
    ERRCONTADOR23=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 94)
    ERRMAXHORADIA=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 95)
    CACHELINHAS=$(cat "${ARQUIVOCACHESTAT}" | cut -d "%" -f 96)
  fi

  rm "${ARQUIVOGNUPLOTDATA}" &>> /dev/null

  NUMEROACERTOS=$(cat "${ARQUIVOLOGTEMP}" | grep "Eval: acertou." | wc -l)

  echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"

  NUMEROERROS=$(cat "${ARQUIVOLOGTEMP}" | grep "Eval: errou." | wc -l)

  echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"

  TOTALRESPOSTAS=$(echo "${NUMEROACERTOS} + ${NUMEROERROS}" | bc)

  PERCENTUALACERTO=$(echo "100 * ${NUMEROACERTOS} / ${TOTALRESPOSTAS}" | bc -l)

  PERCENTUALERRO=$(echo "100 * ${NUMEROERROS} / ${TOTALRESPOSTAS}" | bc -l)

  echo "1 Acertos ${NUMEROACERTOS} 2" > "${ARQUIVOGNUPLOTDATA}"

  echo "2 Erros ${NUMEROERROS} 7" >> "${ARQUIVOGNUPLOTDATA}"

  if [ "${NUMEROACERTOS}" -gt "${NUMEROERROS}" ]; then MAXACERTOSERROS=$NUMEROACERTOS; else MAXACERTOSERROS=$NUMEROERROS; fi

  if [ "${MAXACERTOSERROS}" -gt "0" ]; then gnuplot -p -e "set xtics font 'Verdana,10'; set yrange [0:]; set boxwidth 0.3; set style fill solid; set ytics 0,${MAXACERTOSERROS}; set terminal png size 800,600; set title 'Comparativo de acertos e erros.'; show title; set output '${ARQUIVOGRAFICO}'; plot '${ARQUIVOGNUPLOTDATA}' using 1:3:4:xtic(2) notitle with boxes linecolor variable" &>> /dev/null; else cp "${IMAGEGRAPH0}" "${ARQUIVOGRAFICO}" &>> /dev/null; fi

  echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"

  if [ "${MAXACERTOSERROS}" -gt "0" ]; then gnuplot -p -e "Ai=0.0; yi=0.0; yi2=0.0; ang(x)=x*360.0/${TOTALRESPOSTAS}; set size square; set xrange [-1:1.5]; set yrange [-1.25:1.25]; set boxwidth 0.3; set style fill solid 1; unset border; unset tics; unset key; set terminal png size 800,600; set title 'Comparativo de acertos e erros.'; show title; set output '${ARQUIVOGRAFICOPIZZA}'; plot '${ARQUIVOGNUPLOTDATA}' u (0):(0):(1):(Ai):(Ai=Ai+ang(\$3)):(\$4) with circle linecolor var, '${ARQUIVOGNUPLOTDATA}' u (1.5):(yi=yi+0.5/\$3):(\$2) w labels, '${ARQUIVOGNUPLOTDATA}' u (1.3):(yi2=yi2+0.5/\$3):(\$4) w p pt 5 ps 2 linecolor var" &>> /dev/null; else cp "${IMAGEGRAPH0}" "${ARQUIVOGRAFICOPIZZA}" &>> /dev/null; fi

  echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"

  TOTALLINHAS=$(expr $(cat "${ARQUIVOLOGTEMP}" | wc -l) - $CACHELINHAS)
  CONTADORLINHAS="0";

  rm "${ARQUIVOGNUPLOTDATA2}" &>> /dev/null
  rm "${ARQUIVOGNUPLOTDATA3}" &>> /dev/null
  rm "${ARQUIVOGNUPLOTDATA4}" &>> /dev/null
  rm "${ARQUIVOGNUPLOTDATA5}" &>> /dev/null
  rm "${ARQUIVOGNUPLOTDATA6}" &>> /dev/null
  rm "${ARQUIVOGNUPLOTDATA7}" &>> /dev/null

  ONCE="0"; ONCE2="0"; ONCE3="0"

  while read -r LINHA; do CONTADORLINHAS=$(expr "${CONTADORLINHAS}" + 1); if [ "${CONTADORLINHAS}" -le "${CACHELINHAS}" ]; then echo -ne "\b"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne "${CHARPROGRESS}"; continue; fi; if [ "${ONCE3}" == "0" ]; then echo -n "       "; ONCE3="1"; fi; PERCENTUAL=$(echo "100 * $(expr ${CONTADORLINHAS} - ${CACHELINHAS} - 1) / ${TOTALLINHAS}" | bc -l | awk '{printf "%.2f %%", $1}'); if [ $(echo "${PERCENTUAL}" | cut -d "." -f 1 | wc -c) == "1" ]; then echo -ne "\b\b\b\b\b\b\b"; else if [ $(echo "${PERCENTUAL}" | cut -d "." -f 1 | wc -c) == "2" ]; then echo -ne "\b\b\b\b\b\b\b\b"; if [ "${ONCE}" == "0" ]; then echo -n " "; ONCE="1"; fi; else echo -ne "\b\b\b\b\b\b\b\b\b"; if [ "${ONCE2}" == "0" ]; then echo -n " "; ONCE2="1"; fi; fi; fi; echo -n "${PERCENTUAL}"; case "${CHARPROGRESS}" in "") CHARPROGRESS="-";; "-") CHARPROGRESS="\\";; "\\") CHARPROGRESS="|";; "|") CHARPROGRESS="/";; "/") CHARPROGRESS="-";; *) ;; esac; echo -ne " ${CHARPROGRESS}"

    ERRLINHA="${LINHAR2}"; LINHAR2="${LINHAR3}"; LINHAR3="${LINHAR4}"; LINHAR4="${LINHA}";

    if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Mon" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Tue" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Wed" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Thu" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Fri" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sat" ] || [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sun" ]; then

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Mon" ]; then CONTADORMON=$(echo "${CONTADORMON} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Tue" ]; then CONTADORTUE=$(echo "${CONTADORTUE} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Wed" ]; then CONTADORWED=$(echo "${CONTADORWED} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Thu" ]; then CONTADORTHU=$(echo "${CONTADORTHU} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Fri" ]; then CONTADORFRI=$(echo "${CONTADORFRI} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sat" ]; then CONTADORSAT=$(echo "${CONTADORSAT} + 1" | bc); else

      if [ $(echo "${LINHA}" | cut -d " " -f 1) == "Sun" ]; then CONTADORSUN=$(echo "${CONTADORSUN} + 1" | bc); fi

      fi; fi; fi; fi; fi; fi

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Jan" ]; then CONTADORJAN=$(echo "${CONTADORJAN} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Feb" ]; then CONTADORFEB=$(echo "${CONTADORFEB} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Mar" ]; then CONTADORMAR=$(echo "${CONTADORMAR} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Apr" ]; then CONTADORAPR=$(echo "${CONTADORAPR} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "May" ]; then CONTADORMAY=$(echo "${CONTADORMAY} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Jun" ]; then CONTADORJUN=$(echo "${CONTADORJUN} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Jul" ]; then CONTADORJUL=$(echo "${CONTADORJUL} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Aug" ]; then CONTADORAUG=$(echo "${CONTADORAUG} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Sep" ]; then CONTADORSEP=$(echo "${CONTADORSEP} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Oct" ]; then CONTADOROCT=$(echo "${CONTADOROCT} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Nov" ]; then CONTADORNOV=$(echo "${CONTADORNOV} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Dec" ]; then CONTADORDEC=$(echo "${CONTADORDEC} + 1" | bc); fi

      fi; fi; fi; fi; fi; fi; fi; fi; fi; fi; fi

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "00" ]; then CONTADOR00=$(echo "${CONTADOR00} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "01" ]; then CONTADOR01=$(echo "${CONTADOR01} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "02" ]; then CONTADOR02=$(echo "${CONTADOR02} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "03" ]; then CONTADOR03=$(echo "${CONTADOR03} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "04" ]; then CONTADOR04=$(echo "${CONTADOR04} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "05" ]; then CONTADOR05=$(echo "${CONTADOR05} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "06" ]; then CONTADOR06=$(echo "${CONTADOR06} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "07" ]; then CONTADOR07=$(echo "${CONTADOR07} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "08" ]; then CONTADOR08=$(echo "${CONTADOR08} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "09" ]; then CONTADOR09=$(echo "${CONTADOR09} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "10" ]; then CONTADOR10=$(echo "${CONTADOR10} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "11" ]; then CONTADOR11=$(echo "${CONTADOR11} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "12" ]; then CONTADOR12=$(echo "${CONTADOR12} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "13" ]; then CONTADOR13=$(echo "${CONTADOR13} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "14" ]; then CONTADOR14=$(echo "${CONTADOR14} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "15" ]; then CONTADOR15=$(echo "${CONTADOR15} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "16" ]; then CONTADOR16=$(echo "${CONTADOR16} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "17" ]; then CONTADOR17=$(echo "${CONTADOR17} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "18" ]; then CONTADOR18=$(echo "${CONTADOR18} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "19" ]; then CONTADOR19=$(echo "${CONTADOR19} + 1" | bc); else

      if [ $(echo "${LINHA}" | sed 's/  / /g' | cut -d " " -f 4 | cut -d ":" -f 1) == "20" ]; then CONTADOR20=$(echo "${CONTADOR20} + 1" | bc); else

