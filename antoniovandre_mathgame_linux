      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "May" ]; then ERRCONTADORMAY=$(echo "${ERRCONTADORMAY} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Jun" ]; then ERRCONTADORJUN=$(echo "${ERRCONTADORJUN} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Jul" ]; then ERRCONTADORJUL=$(echo "${ERRCONTADORJUL} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Aug" ]; then ERRCONTADORAUG=$(echo "${ERRCONTADORAUG} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Sep" ]; then ERRCONTADORSEP=$(echo "${ERRCONTADORSEP} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Oct" ]; then ERRCONTADOROCT=$(echo "${ERRCONTADOROCT} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | sed 's/  / /g' | cut -d " " -f 2) == "Nov" ]; then ERRCONTADORNOV=$(echo "${ERRCONTADORNOV} + 1" | bc); else

