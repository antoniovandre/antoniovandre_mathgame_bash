      if [ $(echo "${ERRLINHA}" | cut -d " " -f 1) == "Fri" ]; then ERRCONTADORFRI=$(echo "${ERRCONTADORFRI} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | cut -d " " -f 1) == "Sat" ]; then ERRCONTADORSAT=$(echo "${ERRCONTADORSAT} + 1" | bc); else

      if [ $(echo "${ERRLINHA}" | cut -d " " -f 1) == "Sun" ]; then ERRCONTADORSUN=$(echo "${ERRCONTADORSUN} + 1" | bc); fi

      fi; fi; fi; fi; fi; fi

