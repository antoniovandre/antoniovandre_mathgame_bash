  MAXHORADIA=$(if [ "${MAXHORADIA}" -lt "10" ]; then echo -n $(expr "${MAXHORADIA}" + 1); else echo "${MAXHORADIA} * 1.1" | bc | cut -d "." -f 1; fi)

