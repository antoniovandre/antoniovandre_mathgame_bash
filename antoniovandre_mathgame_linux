  ERRMAXHORADIA=$(if [ "${ERRMAXHORADIA}" -lt "10" ]; then echo -n $(expr "${ERRMAXHORADIA}" + 1); else echo "${ERRMAXHORADIA} * 1.1" | bc | cut -d "." -f 1; fi)

  if [ "${MAXDIASEMANA}" -gt "0" ]; then gnuplot -p -e "set xtics font 'Verdana,10'; set yrange [0:${MAXDIASEMANA}]; set boxwidth 0.3; set style fill solid; set terminal png size 800,600; set xtics rotate by 90 right; unset ytics; set title 'Distribuição semanal de uso do game.'; show title; set output '${ARQUIVOGRAFICOTEMPODISTSEM}'; plot '${ARQUIVOGNUPLOTDATA2}' using 2:xticlabels(1) notitle with boxes linecolor rgb \"gray\", '' using 0:2:2 notitle with labels font 'Verdana,7'"  &>> /dev/null; else cp "${IMAGEGRAPH0}" "${ARQUIVOGRAFICOTEMPODISTSEM}" &>> /dev/null; fi

